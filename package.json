{
  "name": "@djforth/geolocation_helper",
  "version": "1.0.0",
  "description": "A small Geolocation api helper",
  "main": "index.js",
  "scripts": {
    "build": "babel src -x '.js'  -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps  -d lib",
    "lint": "eslint src/**",
    "lint:tests": "eslint spec/**",
    "test": "npm run lint && asset-pipeline-karma",
    "test:watch": "asset-pipeline-karma -w",
    "test:mac": "asset-pipeline-karma -b"
  },
  "assets": {
    "assets_in": "./src",
    "assets_out": "./lib",
    "karma": {
      "input": "./spec"
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/geolocation_helper.git"
  },
  "keywords": [
    "Geolocation",
    "HTML5",
    "Longitude",
    "latitude",
    "locations",
    "html5"
  ],
  "author": "A Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/geolocation_helper/issues"
  },
  "homepage": "https://github.com/djforth/geolocation_helper#readme",
  "devDependencies": {
    "@djforth/ap_karma": "^1.0.0",
    "@djforth/morse-jasmine": "^1.0.8",
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "eslint": "^2.3.0",
    "eslint-config-google": "^0.4.0",
    "estraverse-fb": "^1.3.1",
    "lodash": "^4.6.1"
  },
  "peerDependencies": {
    "lodash": "^4.6.1"
  }
}
